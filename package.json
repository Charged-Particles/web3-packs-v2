{
  "name": "@charged-particles/web3-packs-v2",
  "version": "0.1.0",
  "description": "Ethereum Contracts for Web3-Packs V2",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=20.17.0",
    "npm": ">=10.8.2"
  },
  "scripts": {
    "reinstall": "rm -rf node_modules && rm -f yarn.lock && yarn clean && yarn",
    "clean": "rm -rf build cache coverage coverage.json test-results.xml",
    "clean-deployed": "rm -rf abis && rm -f .openzeppelin/unknown-31337.json && rm -f .openzeppelin/kovan.json",
    "clean-test": "rm -rf abis .openzeppelin/${npm_config_network:-'unknown-31337'}.json deployments/${npm_config_network:-hardhat}/*.json test-results.xml",
    "verify": "yarn hint && yarn test",
    "compile": "hardhat --show-stack-traces --max-memory 8192 compile",
    "test": "yarn hardhat test --deploy-fixture --network hardhat ./tests/Web3PacksMode.test.ts",
    "test-only": "yarn clean-test && hardhat test --show-stack-traces",
    "watch-test": "hardhat watch test",
    "hint": "solhint \"contracts/**/*.sol\"",
    "coverage": "yarn clean-test && hardhat --show-stack-traces coverage --temp coverage_build && yarn clean-test",
    "gas": "REPORT_GAS=true hardhat test",
    "start": "hardhat node --port 8545",
    "deploy": "hardhat deploy --tags mode_packs --network",
    "erc20": "hardhat deploy --tags ERC20Mintable --network",
    "rewards": "hardhat deploy --tags DistroRewards --network"
  },
  "dependencies": {
    "@balancer-labs/sdk": "^1.1.6",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.8",
    "@openzeppelin/contracts": "^4.7.3",
    "@uniswap/sdk-core": "^3.1.1",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "@uniswap/v3-core": "^1.0.1",
    "@uniswap/v3-periphery": "^1.4.3",
    "@uniswap/v3-sdk": "^3.9.0"
  },
  "devDependencies": {
    "@charged-particles/charged-js-sdk": "https://github.com/Charged-Particles/charged-js-sdk.git",
    "@nomicfoundation/hardhat-chai-matchers": "1.0.6",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-etherscan": "3.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/cli": "^2.8.0",
    "@truffle/hdwallet-provider": "^1.0.34",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "chai": "^4.3.7",
    "chai-ethers": "^0.0.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.0.0",
    "eslint-plugin-jest": "^24.0.1",
    "ethereum-waffle": "^3.0.0",
    "ethers": "^5.7.2",
    "hardhat": "^2.22.6",
    "hardhat-abi-exporter": "^2.0.6",
    "hardhat-deploy": "^0.11.20",
    "hardhat-deploy-ethers": "^0.3.0-beta.7",
    "hardhat-gas-reporter": "^1.0.4",
    "hardhat-watcher": "^2.1.1",
    "lodash": "^4.17.21",
    "solc": "0.8.17",
    "solhint": "^3.0.0",
    "solidity-coverage": "^0.7.4",
    "solidity-docgen": "^0.5.3",
    "ts-node": "^10.9.1",
    "typechain": "^8.3.2",
    "typescript": "^4.9.4"
  }
}
